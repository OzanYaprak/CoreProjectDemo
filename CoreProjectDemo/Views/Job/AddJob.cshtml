@model Job

@{
    ViewData["Title"] = "AddJob";
}


<form asp-controller="Job" asp-action="AddJob" method="post">
    <div asp-validation-summary="All"></div>

    <div class="form-group">
        <label asp-for="JobName">Meslek adı</label>
        <input asp-for="JobName" type="text" class="form-control" id="jobname">
    </div>
    <button type="submit" class="btn btn-primary mt-3">Kaydet</button>
</form>